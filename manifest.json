{
  "manifest_version": 3,
  "name": "Tab Organizer",
  "version": "1.0",
  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "action": {
    "default_icon": {
      "16": "icons/16.png",
      "32": "icons/32.png",
      "48": "icons/48.png",
      "128": "icons/128.png"
    },
    "default_title": "Tab organizer 2",
    "default_popup": "popup/popup.html"
  },
  "background": {
    "service_worker": "scripts/service-worker.js"
  },
  "permissions": [
    "activeTab",
    "tabs"
  ]
}